<h1>Add a New Menu Item</h1>
<%= form_for @menu_item do |f| %>
  <%= f.label :name %>
  <%= errors_for(@menu_item, :name) %>
  <%= f.text_field :name %>
  <%= f.label :price %>
  <%= errors_for(@menu_item, :price) %>
  <%= f.text_field :price %>
  <%= f.label :image %>
  <%= errors_for(@menu_item, :image) %>
  <%= f.file_field :image %>
  <%= f.submit "Add Item" %>
<% end %>
